{
  "name": "json-schema-2-xlsx",
  "version": "1.0.0",
  "description": "Library for automatically converting JSON Schema structures to Excel (XLSX) spreadsheets in a structured way",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "test": "vitest",
    "build": "tsc",
    "dev": "tsx --watch src/example.ts",
    "prepublishOnly": "npm run build"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "json-schema",
    "xlsx",
    "excel",
    "converter",
    "exceljs",
    "typescript",
    "json",
    "schema",
    "excel-generator"
  ],
  "author": "PedrosoOrganization",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/PedrosoOrganization/json-schema-2-xlsx"
  },
  "bugs": {
    "url": "https://github.com/PedrosoOrganization/json-schema-2-xlsx/issues"
  },
  "homepage": "https://github.com/PedrosoOrganization/json-schema-2-xlsx#readme",
  "devDependencies": {
    "@types/json-schema": "^7.0.15",
    "tsx": "^4.19.3",
    "typescript": "^5.8.3",
    "vitest": "^3.1.2"
  },
  "dependencies": {
    "exceljs": "^4.4.0",
    "json-schema": "^0.4.0",
    "jsonpath-plus": "^10.3.0"
  }
}
